{
  "private": true,
  "scripts": {
    "fable": "dotnet run -c Release -p src/dotnet/Fable.Compiler",
    "build-tests": "yarn fable fable-splitter --force-pkgs --args \"-c tests/splitter.config.js\"",
    "postbuild-tests": "yarn test",
    "test": "mocha build/tests --reporter dot -t 10000",
    "build-repl": "yarn fable fable-splitter --args \"-c src/dotnet/Fable.Repl/splitter.config.js --commonjs\"",
    "build-repl-modules": "yarn fable fable-splitter --args \"-c src/dotnet/Fable.Repl/splitter.config.js \"",
    "build-repl-es6": "yarn fable fable-splitter --args \"-c src/dotnet/Fable.Repl/splitter.config.js --es6\"",
    "bundle-repl": "rollup src/dotnet/Fable.Repl/out/Main.js --file src/dotnet/Fable.Repl/bundle/bundle.js --format iife --name Fable",
    "minify-repl": "uglifyjs src/dotnet/Fable.Repl/bundle/bundle.js -o src/dotnet/Fable.Repl/bundle/bundle.min.js -c -m",
    "build-bench": "yarn fable fable-splitter --args \"-c src/dotnet/Fable.Repl/bench/splitter.config.js --commonjs\"",
    "start-bench": "cd src/dotnet/Fable.Repl/bench && node out/app ../metadata/ test_script.fs",
    "build-bench2": "yarn fable fable-splitter --args \"-c src/dotnet/Fable.Repl/bench2/splitter.config.js\"",
    "start-bench2": "cd src/dotnet/Fable.Repl/bench2 && node out/app ../metadata/ test_script.fs out/test_script.js",
    "test-bench2": "node src/dotnet/Fable.Repl/bench2/out/test_script.js"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "7.0.0",
    "@babel/standalone": "^7.0.0",
    "@babel/template": "^7.0.0",
    "babel-loader": "^8.0.2",
    "fable-loader": "2.0.0-beta-001",
    "fable-splitter": "2.0.0-beta-002",
    "mocha": "^5.2.0",
    "rollup": "^0.65.2",
    "terser": "^3.8.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "uglify-js": "^3.4.9",
    "webpack": "^4.17.1",
    "webpack-command": "^0.4.1"
  }
}
