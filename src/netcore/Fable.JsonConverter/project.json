{
  "version": "1.0.0",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "../../nuget/Fable.JsonConverter/Fable.JsonConverter.fs"
      ]
    },
    "define": [
      "DOTNETCORE",
      "TRACE"
    ],
    "nowarn": [
      "44",
      "2003"
    ],
    "xmlDoc": true,
    "delaySign": true,
    "warningsAsErrors": true,
    "additionalArguments": [
      "--fullpaths",
      "--flaterrors"
    ]
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
    "Newtonsoft.Json": "9.0.1"
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "frameworks": {
    "netstandard1.6": {
    }
  }
}